{
  "name": "nanbox",
  "version": "1.0.3",
  "description": "Encode hidden data within NaN IEEE 754 floats",
  "scripts": {
    "build": "babel src -d dist",
    "prepublish": "npm run build",
    "lint": "eslint .",
    "release": "np",
    "test": "nyc ava"
  },
  "main": "./dist/index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/philihp/nanbox.git"
  },
  "keywords": [
    "nan",
    "hidden",
    "payload"
  ],
  "author": "Philihp Busby <philihp@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/philihp/nanbox/issues"
  },
  "homepage": "https://github.com/philihp/nanbox#readme",
  "devDependencies": {
    "@ava/babel": "1.0.1",
    "@babel/cli": "7.10.1",
    "@babel/core": "7.10.2",
    "@babel/preset-env": "7.10.2",
    "@babel/register": "7.10.1",
    "ava": "3.8.2",
    "babel-eslint": "10.1.0",
    "eslint": "7.2.0",
    "eslint-config-airbnb-base": "14.1.0",
    "eslint-config-prettier": "6.11.0",
    "eslint-plugin-import": "2.20.2",
    "eslint-plugin-prettier": "3.1.3",
    "husky": "4.2.5",
    "lint-staged": "10.2.9",
    "nyc": "15.1.0",
    "prettier": "2.0.5"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,json}": [
      "eslint --fix"
    ]
  },
  "prettier": {
    "singleQuote": true,
    "semi": false,
    "trailingComma": "es5"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "eslintConfig": {
    "extends": [
      "airbnb-base",
      "plugin:prettier/recommended"
    ],
    "parser": "babel-eslint",
    "rules": {
      "no-bitwise": "off"
    }
  },
  "ava": {
    "babel": true,
    "require": [
      "./test/_register.js"
    ],
    "files": [
      "test/**/*.test.js"
    ]
  },
  "dependencies": {}
}
