{
  "name": "nanbox",
  "version": "1.0.3",
  "description": "Encode hidden data within NaN IEEE 754 floats",
  "scripts": {
    "build": "babel src -d dist",
    "prepublish": "npm run build",
    "lint": "eslint .",
    "release": "np",
    "test": "jest",
    "test:coverage": "jest --coverage"
  },
  "main": "./dist/index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/philihp/nanbox.git"
  },
  "keywords": [
    "nan",
    "hidden",
    "payload"
  ],
  "author": "Philihp Busby <philihp@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/philihp/nanbox/issues"
  },
  "homepage": "https://github.com/philihp/nanbox#readme",
  "devDependencies": {
    "@babel/cli": "7.10.1",
    "@babel/core": "7.10.2",
    "@babel/preset-env": "7.10.2",
    "@babel/register": "7.10.1",
    "@philihp/eslint-config": "2.0.3",
    "husky": "4.2.5",
    "jest": "26.0.1",
    "lint-staged": "10.2.11",
    "prettier": "2.0.5"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,json}": [
      "eslint --fix"
    ]
  },
  "prettier": {
    "singleQuote": true,
    "semi": false,
    "trailingComma": "es5"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "eslintConfig": {
    "extends": [
      "@philihp"
    ],
    "rules": {
      "no-bitwise": "off",
      "unicorn/number-literal-case": 0,
      "unicorn/no-fn-reference-in-iterator": 0
    },
    "settings": {
      "jest": {
        "version": 26
      }
    }
  },
  "dependencies": {}
}
